{
  "name": "dev-bot-2",
  "version": "1.0.0",
  "description": "Remake of DevBot in typescript for RubyRealms",
  "repository": {
    "type": "git",
    "url": "https://github.com/alihsaas/dev-bot-2"
  },
  "engine": {
  	"node": "14.7.0"
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node -r dotenv/config ./build/index.js",
    "watch-node": "nodemon build/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/mysql": "^2.15.15",
    "canvas": "^2.6.1",
    "cheerio": "^1.0.0-rc.3",
    "crypto": "^1.0.1",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "node-cmd": "^3.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/express": "^4.17.7",
    "@types/jest": "^22.2.2",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/ws": "^7.2.6",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "jest": "^22.4.3",
    "ts-jest": "^22.0.4",
    "ts-mockito": "^2.3.0",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.2"
  }
}
